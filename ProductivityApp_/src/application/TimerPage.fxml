<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Label text="TIMER!" textAlignment="CENTER">
         <VBox.margin>
            <Insets left="220.0" />
         </VBox.margin>
         <font>
            <Font size="47.0" />
         </font>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </Label>
      <Label alignment="CENTER" prefHeight="36.0" prefWidth="100.0" text="Set Time:" textAlignment="CENTER">
         <font>
            <Font size="17.0" />
         </font>
         <VBox.margin>
            <Insets bottom="30.0" left="60.0" right="60.0" top="60.0" />
         </VBox.margin>
      </Label>
      <Button fx:id="loginButtonID" mnemonicParsing="false" onAction="#loginButton" prefHeight="28.0" prefWidth="92.0" text="Start Timer">
         <VBox.margin>
            <Insets left="250.0" top="150.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
